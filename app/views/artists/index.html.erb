<h1>Artists</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.name %></td>
        <td><%= artist.age %></td>
        <td><%= artist.title %></td>

      
        <td><%= link_to 'Show', artist %></>
      </tr>
    <% end %>
  </tbody>
</table>



